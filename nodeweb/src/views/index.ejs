<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>GCloud Speech Recognition Node + Socket.io</title>
    <meta name="description" content="Google Speech Recognition Test">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="assets/css/main.css">
</head>

<body>
    <div class="wrapper">
        <h1>Google Speech Recognition Test</h1>

        <audio></audio>

        <br>
        <button id="startRecButton" type="button"> Start recording</button>
        <button id="stopRecButton" type="button"> Stop recording</button>
        <div id="recordingStatus">&nbsp;</div>
        <br>

        <div>
            <p id="ResultText">
                <span class="greyText">No Speech to Text yet
                    <span>
            </p>
        </div>

        <br>
        <br>
    </div>
    <div class="wrapper">
        <h1>Supervised Learning Test</h1>

    <div id="console"></div>
    <!-- Add an image that we will use to test -->
    <!--<img id="img" crossorigin src="https://i.imgur.com/Fi4j15h.jpg" width="227" height="227"/>-->
        <video autoplay playsinline muted id="webcam" width="224" height="224"></video>
        <button id="class-a">Add A</button>
        <button id="class-b">Add B</button>
        <button id="class-c">Add C</button>
        <button id="save_button">Save</button>
        <!--<button id="load_button">Load</button>-->
        <input id="load_button" type='file' accept='.json'>
        <img id='output'>
    </div>

    <!-- Nlp -->
    <script src="https://unpkg.com/compromise@latest/builds/compromise.min.js"></script>

    <!-- Socket -->
    <script src="assets/js/socket.io.js"></script>

    <!-- Client -->
    <script src="assets/js/client.js"></script>

    <!-- TensorflowJS Model -->
    <script src="https://unpkg.com/@tensorflow/tfjs"></script>
    <script src="https://unpkg.com/@tensorflow-models/mobilenet"></script>
    <script src="https://unpkg.com/@tensorflow-models/knn-classifier"></script>
</body>

</html>
